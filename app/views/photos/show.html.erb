<div class="col-md-8 col-md-offset-2">
  <%= @photo.title %><br>
  <%= image_tag @photo.url, class: "img-responsive" %>
  <%= link_to "Index photo", photos_path, class: "btn btn-default show-btn" %>
  <%= link_to "New photo", new_photo_path, class: "btn btn-default show-btn" %>
  <%= link_to "Edit photo", edit_photo_path(@photo), class: "btn btn-default show-btn" %>
  <%= link_to "Delete photo", photo_path(@photo), method: :delete, class: "btn btn-default show-btn" %>
</div>